# Copy this file to .env and fill in your real keys (never commit a real key)

# Google Gemini (AI content generation)
GEMINI_API_KEY=YOUR_GEMINI_KEY_HERE

# Google Maps (geocoding, distance + optional places details)
GOOGLE_API_KEY=YOUR_GOOGLE_KEY_HERE

# FBI / data.gov API key (crime data)
FBI_API_KEY=YOUR_FBI_KEY_HERE

# Server port
PORT=3000

# Cache TTL (seconds) for property lookups (default 900 = 15m)
CACHE_TTL_SECONDS=900

# Local path or URL to Zillow ZIP-level ZHVI CSV (leave blank to use built-in default)
ZILLOW_ZIP_ZHVI_CSV=

# Set to 1 to skip dataset prefetch on startup (useful for cold-start performance or debugging)
# SKIP_PREFETCH=1